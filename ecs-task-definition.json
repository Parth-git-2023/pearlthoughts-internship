{
  "family": "parth-strapi-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::607700977843:role/ecs-task-execution-role-p",
  "containerDefinitions": [
    {
      "name": "strapi",
      "image": "IMAGE_PLACEHOLDER",
      "portMappings": [
        {
          "containerPort": 1337,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
